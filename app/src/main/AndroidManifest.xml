<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="sph.hrt.misplantillas">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MisPlantillas"
        tools:targetApi="31">
        <activity
            android:name=".NavDrawerSub.NavDrawerSubActivity"
            android:exported="false"
            android:theme="@style/Theme.MisPlantillas.NoActionBar"
            >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".tabSanti.SantiTabActivity"
            android:exported="true">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".tabActivity.TabActivity"
            android:exported="false"
            android:theme="@style/Theme.MisPlantillas.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".NavDrawer.NavDrawerActivity"
            android:exported="false"
            android:theme="@style/Theme.MisPlantillas.NoActionBar" />
        <activity
            android:name=".FullScreen.FullActivity"
            android:exported="false"
            android:theme="@style/Theme.MisPlantillas.NoActionBar" />
        <activity
            android:name=".bottomActivity.BottomActivity"
            android:exported="false"
            android:theme="@style/Theme.MisPlantillas.NoActionBar" />
        <activity
            android:name=".basicActivity.BasicActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>